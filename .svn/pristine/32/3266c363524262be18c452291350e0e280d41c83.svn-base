<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>Registracija</title>
    </head>
    <body>
        <form method="post" action="registration.jsp">
            <center>
            <table border="1" width="30%" cellpadding="5">
                <thead>
                    <tr>
                        <th colspan="2">Vnos registracijskih podatkov</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Ime: </td>
                        <td><input type="text" name="fname" value="" /></td>
                    </tr>
                    <tr>
                        <td>Priimek: </td>
                        <td><input type="text" name="lname" value="" /></td>
                    </tr>
                    <tr>
                        <td>Email: </td>
                        <td><input type="text" name="email" value="" /></td>
                    </tr>
                    <tr>
                        <td>Uporabniško ime: </td>
                        <td><input type="text" name="uname" value="" /></td>
                    </tr>
                    <tr>
                        <td>Geslo: </td>
                        <td><input type="password" name="pass" value="" /></td>
                    </tr>
                    <tr>
                        <td><input type="submit" value="Registracija" /></td>
                        <td><input type="reset" value="Ponastavi" /></td>
                    </tr>
                    <tr>
                        <td colspan="2">Ste že registrirani? Prijava <a href="index.jsp">tukaj.</a></td>
                    </tr>
                </tbody>
            </table>
            </center>
        </form>
    </body>
</html>